# 0 - fire
# 1 - water
# 2 - earth
# 3 - air 
# 4 - light 
# 5 - dark 

execute if score .type mcrpg matches 1 run function mcrpg:item/skill/magic/fire
execute if score .type mcrpg matches 2 run function mcrpg:item/skill/magic/water
execute if score .type mcrpg matches 3 run function mcrpg:item/skill/magic/air
execute if score .type mcrpg matches 4 run function mcrpg:item/skill/magic/earth
execute if score .type mcrpg matches 5 run function mcrpg:item/skill/magic/light
execute if score .type mcrpg matches 6 run function mcrpg:item/skill/magic/dark
execute if score .type mcrpg matches 7 run function mcrpg:item/skill/magic/ender